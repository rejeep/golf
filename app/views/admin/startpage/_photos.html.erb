<div class="span7">
  <h2><%= link_to 'Photos', admin_photos_path %></h2>
  <ul class="media-grid">
    <% @photos.each do |photo| %>
      <li>
        <%= link_to image_tag(photo.image.url(:thumb)), photo.image.url, :rel => 'photo', :class => 'fancybox' %>
      </li>
    <% end %>
  </ul>
</div>
